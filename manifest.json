{
	"manifest_version": 3,
	"name": "ChatGPT Prompt Table of Contents",
	"version": "1.1",
	"description": "Shows a sidebar as a Table of Contents of prompts in ChatGPT conversation.",
	"permissions": [
		"activeTab",
		"tabs"
	],
	"content_scripts": [
		{
			"matches": [
				"https://chat.openai.com/chat/*",
				"https://chatgpt.com/*"
			],
			"js": [
				"content.js"
			],
			"css": [
				"sidebar.css"
			],
			"run_at": "document_idle"
		}
	],
	"icons": {
		"16": "icons/icon16.png",
		"32": "icons/icon32.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	  }
}